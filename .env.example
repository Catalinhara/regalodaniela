# Environment variables for ClaraMente
# Copy this file to .env and update the values

# OpenRouter API Key (for LLM features)
OPENROUTER_API_KEY=your-openrouter-api-key-here

# JWT Secrets (CHANGE THESE IN PRODUCTION!)
# Generate random strings with: python -c "import secrets; print(secrets.token_urlsafe(32))"
JWT_SECRET_KEY=dev-secret-key-change-in-production-min-32-chars-long-random-string
JWT_REFRESH_SECRET_KEY=dev-refresh-secret-key-change-in-production-min-32-chars-long-random-string
